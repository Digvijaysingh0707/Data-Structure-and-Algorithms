class Solution {
    public int combinationSum4(int[] nums, int target) {
        Arrays.sort(nums);
        int dp[]= new int[target+1];
        dp[0]=1;
        for(int i=1;i<=target;i++){
            loop:for(int j=0;j<nums.length;j++){
                if(i>=nums[j]){
                    dp[i]+=dp[i-nums[j]];
                }
                else break loop;
            }
        }
        return dp[target];
    }
}
